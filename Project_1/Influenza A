      },
      "source": [
        "classification = []\n",
        "for i in newdf.standard_value:\n",
        "    if float(i) >= 10000:\n",
        "        classification.append(\"Inactive\")\n",
        "    elif float(i) <= 1000:\n",
        "        classification.append(\"Active\")\n",
        "    else: \n",
        "        classification.append(\"intermediate\")\n"
      ],
      "execution_count": 22,
      "outputs": []
    },
