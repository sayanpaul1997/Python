        "for i in newdf.standard_value:\n",
        "    if float(i) >= 10000:\n",
        "        print(\"Inactive\")\n",
        "    elif float(i) <= 1000:\n",
        "        print(\"Active\")\n",
        "    else: \n",
        "        print(\"intermediate\")\n"
      ],
